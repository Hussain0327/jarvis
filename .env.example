# Jarvis environment variables
# Copy to .env and fill in values.

# Database
JARVIS_DATABASE__HOST=localhost
JARVIS_DATABASE__PORT=5432
JARVIS_DATABASE__NAME=jarvis
JARVIS_DATABASE__USER=jarvis
JARVIS_DATABASE__PASSWORD=jarvis_dev

# Application
JARVIS_DEBUG=false
JARVIS_LOG_LEVEL=INFO

# Perception (optional overrides)
# JARVIS_PERCEPTION_VEHICLE_DETECTOR__DEVICE=cuda
# JARVIS_PERCEPTION_PLATE_DETECTOR__DEVICE=cuda
